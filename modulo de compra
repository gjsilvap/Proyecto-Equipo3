    private void jLabelBuscar1MouseClicked(java.awt.event.MouseEvent evt) {                                           
        if(this.jtextCedulaProo.getText().equals("")){
            JOptionPane.showMessageDialog(new JFrame(),"Rellene el campo de identidad","Mensaje",JOptionPane.INFORMATION_MESSAGE);
        }
        else{
        try{
         Conexion conec = new Conexion();
         conec.ConectarBDatos();
         conec.sql="Select proveedor.* from proveedor where rif_o_ced='"+this.boxidentidad.getSelectedItem()+jtextCedulaProo.getText()+"'";
         conec.tb=conec.stm.executeQuery(conec.sql);
          if(conec.tb.next()){
              this.jfieldNombreProo.setText(conec.tb.getString("nombre_pro"));
              this.jfieldDirecProo.setText(conec.tb.getString("direccion_pro"));
              this.jtexttlfproo.setText(conec.tb.getString("telf_pro"));
              this.jtextCedulaProo.setEnabled(false);
              this.jLabelEliminar.setEnabled(true);
              this.jLabelModificar.setEnabled(true);
              this.jfieldNombreProo.setEnabled(true);
              this.jfieldDirecProo.setEnabled(true);
              this.jtexttlfproo.setEnabled(true);
              this.jLabelRegistrar1.setEnabled(false);
         }
          else
           {
           JOptionPane.showMessageDialog(new JFrame(),"Cedula no registrada,\nrellene los siguientes campos","Mensaje",JOptionPane.INFORMATION_MESSAGE);
           this.jLabelRegistrar1.setEnabled(true);
           this.jfieldNombreProo.setEnabled(true);
           this.jfieldDirecProo.setEnabled(true);
           this.jtexttlfproo.setEnabled(true);
           }
        }
        catch(Exception e2)
          {
              System.out.println("Error en JDBC");
        }
        }
    }                                          

    private void jLabelRegistrar1MouseClicked(java.awt.event.MouseEvent evt) {                                              
        try
        {
            if((this.jfieldNombreProo.getText().equals("")) || (this.jfieldDirecProo.getText().equals("")) || (this.jtexttlfproo.getText().equals(""))){
                   JOptionPane.showMessageDialog(new JFrame(), "Rellene todos los campos", "Alerta", JOptionPane.WARNING_MESSAGE);
            }
            else{
                Conexion conec = new Conexion();
        conec.ConectarBDatos();
        conec.sql="INSERT INTO proveedor (rif_o_ced, nombre_pro, telf_pro, direccion_pro) VALUES ('"+this.boxidentidad.getSelectedItem()+this.jtextCedulaProo.getText()+"','"+this.jfieldNombreProo.getText()+"','"+this.jtexttlfproo.getText()+"','"+this.jfieldDirecProo.getText()+"')";
        conec.stm.executeUpdate(conec.sql);
        JOptionPane.showMessageDialog(new JFrame(),"Registro incluido","Mensaje",JOptionPane.INFORMATION_MESSAGE);
        this.jtextCedulaProo.setText("");
        this.jfieldNombreProo.setText("");
        this.jtexttlfproo.setText("");
        this.jfieldDirecProo.setText("");
        this.jfieldNombreProo.setEnabled(false);
        this.jtexttlfproo.setEnabled(false);
        this.jfieldDirecProo.setEnabled(false);
        this.jtextCedulaProo.setEnabled(true);
        this.jtextCedulaProo.requestFocus();
        conec.DesconectarBD();           
        }
        }
        catch(Exception e2)
        {
        System.out.println("Error en incluir "+e2);
        }
    }                                             

    private void jLabelRegistrar2MouseClicked(java.awt.event.MouseEvent evt) {                                              
        try
        {
            if((this.jtextprodcom.getText().equals("")) || (this.jtextcantinv.getText().equals(""))
                    || (this.jtextceducom.getText().equals("")) || (this.jtextcostocom.getText().equals(""))){
                   JOptionPane.showMessageDialog(new JFrame(), "Rellene todos los campos", "Alerta", JOptionPane.WARNING_MESSAGE);
            }
            else{
                Conexion conec = new Conexion();
        conec.ConectarBDatos();
        Date fechacompra = new java.sql.Date(new java.util.Date().getTime());
        Integer cant = Integer.parseInt(this.jtextcantinv.getText());
        float costocom = Float.parseFloat(this.jtextcostocom.getText());
        float costotal=Float.parseFloat(this.jtextcostocom.getText())*cant;
        DateFormat formatter = new SimpleDateFormat("yyyy/MM/dd");
        Date fecha = this.jDatecompra.getDate();
        String fechatext = formatter.format(fecha);
        Date strDate = formatter.parse(fechatext);
        conec.sql="INSERT INTO compras(cedula_com, producto_com, cant_prod, costo_comp, costotal_comp, fecha_comp) VALUES ('"+this.boxidentidad.getSelectedItem()+this.jtextceducom.getText()+"','"+this.jtextprodcom.getText()+"','"+this.jtextcantinv.getText()+"','"+costocom+"','"+costotal+"','"+fechatext+"')";
        conec.stm.executeUpdate(conec.sql);
        if(this.jtextprodcom.isEnabled()==true){
        conec.sql="INSERT INTO inventario(producto, cant) VALUES ('"+this.jtextprodcom.getText()+"','"+cant+"')";
        conec.stm.executeUpdate(conec.sql);
        }
        if(this.jtextprodcom.isEnabled()==false){
            conec.sql="SELECT inventario.* from inventario where codigo_inv='"+this.codigoprod.getText()+"'";
            conec.tb=conec.stm.executeQuery(conec.sql);
            if(conec.tb.next()){
                Integer cantant = Integer.parseInt(conec.tb.getString("cant"));
                int cantotal=cantant+cant;
            conec.sql="UPDATE inventario set cant='"+cantotal+"' where codigo_inv='"+this.codigoprod.getText()+"'";
            conec.stm.executeUpdate(conec.sql);
            }
        }
        JOptionPane.showMessageDialog(new JFrame(),"Compra registrada","Mensaje",JOptionPane.INFORMATION_MESSAGE);
        this.jtextceducom.setText("");
        this.jtextcostocom.setText("");
        this.jtextprodcom.setText("");
        this.jtextcantinv.setText("");
        this.codigoprod.setText("");
        this.codigoprod.setEnabled(false);
        this.jtextcostocom.setEnabled(false);
        this.jtextprodcom.setEnabled(false);
        this.jtextcantinv.setEnabled(false);
        this.jDatecompra.setEnabled(true);
        this.jtextceducom.setEnabled(true);
        this.jtextceducom.requestFocus();
        conec.DesconectarBD();           
        }
        }
        catch(Exception e2)
        {
        System.out.println("Error en incluir "+e2);
        }
         
    
   
    }                                             

    private void jLabelEliminarMouseClicked(java.awt.event.MouseEvent evt) {                                            
        try{
            int res= JOptionPane.showConfirmDialog(new JFrame(), "¿Esta seguro de eliminar este registro?","Pregunta",JOptionPane.YES_NO_OPTION,JOptionPane.QUESTION_MESSAGE);
            if(res == 0)
            {
                Conexion conec = new Conexion();
                conec.ConectarBDatos();
                conec.sql="Delete from proveedor where rif_o_ced='"+this.boxidentidad.getSelectedItem()+this.jtextCedulaProo.getText()+"'";
                conec.stm.executeUpdate(conec.sql);
                JOptionPane.showMessageDialog(new JFrame(),"Registro Eliminado","Mensaje",JOptionPane.ERROR_MESSAGE);
                conec.DesconectarBD();
                this.jtextCedulaProo.setText("");
                this.jfieldNombreProo.setText("");
                this.jfieldDirecProo.setText("");
                this.jtexttlfproo.setText("");
                this.jfieldNombreProo.setEnabled(false);
                this.jfieldDirecProo.setEnabled(false);
                this.jtexttlfproo.setEnabled(false);
                this.jtextCedulaProo.setEnabled(true);
                jtextCedulaProo.requestFocus();
            }
        }
        catch(Exception e2){
            System.out.println("Error en eliminar" +e2);
        }               
    }                                           

    private void jLabelModificarMouseClicked(java.awt.event.MouseEvent evt) {                                             
        try{
            Conexion conec = new Conexion();
            conec.ConectarBDatos();
            conec.sql = "UPDATE proveedor set rif_o_ced='"+this.boxidentidad.getSelectedItem()+jtextCedulaProo.getText()+"', nombre_pro='"+jfieldNombreProo.getText()+"', telf_pro='"+jtexttlfproo.getText()+"', direccion_pro='"+jfieldDirecProo.getText()+"' where rif_o_ced='"+this.boxidentidad.getSelectedItem()+jtextCedulaProo.getText()+"'";
            conec.stm.executeUpdate(conec.sql);
            JOptionPane.showMessageDialog(new JFrame(),"Registro Actualizado","Mensaje",JOptionPane.WARNING_MESSAGE);
            this.jtextCedulaProo.setText("");
            this.jfieldNombreProo.setText("");
            this.jfieldDirecProo.setText("");
            this.jtexttlfproo.setText("");
            this.jfieldNombreProo.setEnabled(false);
            this.jfieldDirecProo.setEnabled(false);
            this.jtexttlfproo.setEnabled(false);
            this.jtextCedulaProo.setEnabled(true);	
            jtextCedulaProo.requestFocus();
            conec.DesconectarBD();
        }
        catch(Exception e2){
            System.out.println("Eror al modificar"+e2);
        }                    
    }                                            

