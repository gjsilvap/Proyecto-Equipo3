    private void jfieldNombreProoKeyTyped(java.awt.event.KeyEvent evt) {                                          
                char c=evt.getKeyChar(); 
          if(Character.isDigit(c)) { 
              getToolkit().beep(); 
              evt.consume(); 
          }
    }                                         

    private void jfieldNombreProoFocusGained(java.awt.event.FocusEvent evt) {                                             
        if(this.boxidentidad.getSelectedIndex()==0 || this.boxidentidad.getSelectedIndex()==1 || this.boxidentidad.getSelectedIndex()==2){
            this.jfieldNombreProo.setDocument(new JTextFieldLimit(45));
        }
        if(this.boxidentidad.getSelectedIndex()==3 || this.boxidentidad.getSelectedIndex()==4){
            this.jfieldNombreProo.setDocument(new JTextFieldLimit(45));
        }
    }                                            

    private void jfieldDirecProoFocusGained(java.awt.event.FocusEvent evt) {                                            
        if(this.boxidentidaddos.getSelectedIndex()==0 || this.boxidentidaddos.getSelectedIndex()==1 || this.boxidentidaddos.getSelectedIndex()==2){
            this.jfieldDirecProo.setDocument(new JTextFieldLimit(99));
        }
        if(this.boxidentidaddos.getSelectedIndex()==3 || this.boxidentidaddos.getSelectedIndex()==4){
            this.jfieldDirecProo.setDocument(new JTextFieldLimit(99));
        }
    }                                           

