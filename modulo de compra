    private void buscarprodMouseClicked(java.awt.event.MouseEvent evt) {                                        
        try{
         Conexion conec = new Conexion();
         conec.ConectarBDatos();
         conec.sql="Select inventario.* from inventario where codigo_inv='"+this.codigoprod.getText()+"'";
         conec.tb=conec.stm.executeQuery(conec.sql);
          if(conec.tb.next()){
              this.jtextprodcom.setText(conec.tb.getString("producto"));
              this.jtextcantinv.setText(conec.tb.getString("cant"));
              this.codigoprod.setEnabled(false);
            //  this.jtextprodcom.setEnabled(true);
              this.jtextcantinv.setEnabled(true);
              this.jtextcostocom.setEnabled(true);
              this.jDatecompra.setEnabled(true);
         }
          else
           {
           JOptionPane.showMessageDialog(new JFrame(),"Producto no registrado, rellene los campos para añadir los productos","Mensaje",JOptionPane.INFORMATION_MESSAGE);
            this.jtextprodcom.setEnabled(true);
            this.jtextcantinv.setEnabled(true);
            this.jtextcostocom.setEnabled(true);
            this.jDatecompra.setEnabled(true);
            this.codigoprod.setEnabled(false);
         //   this.codigoprod.requestFocus();
           }
        }
        catch(Exception e2)
          {
              System.out.println("Error en JDBC");
        }
    }                                       

