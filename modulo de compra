    private void jtextceducomKeyTyped(java.awt.event.KeyEvent evt) {                                      
        char c = evt.getKeyChar();
        if (c<'0' || c>'9') evt.consume();
    }                                     

    private void jtextCedulaProoFocusGained(java.awt.event.FocusEvent evt) {                                            
        if(this.boxidentidad.getSelectedIndex()==0 || this.boxidentidad.getSelectedIndex()==1 || this.boxidentidad.getSelectedIndex()==2){
            this.jtextCedulaProo.setDocument(new JTextFieldLimit(8));
        }
        if(this.boxidentidad.getSelectedIndex()==3 || this.boxidentidad.getSelectedIndex()==4){
            this.jtextCedulaProo.setDocument(new JTextFieldLimit(9));
        }
    }                                           

    private void jtextceducomFocusGained(java.awt.event.FocusEvent evt) {                                         
        if(this.boxidentidaddos.getSelectedIndex()==0 || this.boxidentidaddos.getSelectedIndex()==1 || this.boxidentidaddos.getSelectedIndex()==2){
            this.jtextceducom.setDocument(new JTextFieldLimit(8));
        }
        if(this.boxidentidaddos.getSelectedIndex()==3 || this.boxidentidad.getSelectedIndex()==4){
            this.jtextceducom.setDocument(new JTextFieldLimit(9));
        }
    }                                        

    private void jLabelBuscar2MouseClicked(java.awt.event.MouseEvent evt) {                                           
        if(this.jtextceducom.getText().equals("")){
            JOptionPane.showMessageDialog(new JFrame(),"Rellene el campo de identidad","Mensaje",JOptionPane.INFORMATION_MESSAGE);
        }
        else{
        try{
         Conexion conec = new Conexion();
         conec.ConectarBDatos();
         conec.sql="Select proveedor.* from proveedor where rif_o_ced='"+this.boxidentidaddos.getSelectedItem()+jtextceducom.getText()+"'";
         conec.tb=conec.stm.executeQuery(conec.sql);
          if(conec.tb.next()){
              this.jtextceducom.setEnabled(false);
            //  this.jtextprodcom.setEnabled(true);
            // this.jtextcantinv.setEnabled(true);
            // this.jtextcostocom.setEnabled(true);
           //   this.jDatecompra.setEnabled(true);
              this.codigoprod.setEnabled(true);
              this.codigoprod.requestFocus();
         }
          else
           {
           JOptionPane.showMessageDialog(new JFrame(),"Proveedor no registrado,\nrellene los campos a la izquierda","Mensaje",JOptionPane.INFORMATION_MESSAGE);
           this.jtextCedulaProo.requestFocus();
           }
        }
        catch(Exception e2)
          {
              System.out.println("Error en JDBC");
        }
        }
    }                                          

